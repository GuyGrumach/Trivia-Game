<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" minHeight="275.0" minWidth="550.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="280.0" prefWidth="549.0">
            <children>
                <RadioButton fx:id="answer1" layoutX="139.0" layoutY="109.0" mnemonicParsing="false" text="RadioButton">
                    <toggleGroup>
                        <ToggleGroup fx:id="answers" />
                    </toggleGroup></RadioButton>
                <RadioButton fx:id="answer2" layoutX="139.0" layoutY="153.0" mnemonicParsing="false" text="RadioButton" toggleGroup="$answers" />
                <RadioButton fx:id="answer3" layoutX="311.0" layoutY="109.0" mnemonicParsing="false" text="RadioButton" toggleGroup="$answers" />
                <RadioButton fx:id="answer4" layoutX="311.0" layoutY="153.0" mnemonicParsing="false" text="RadioButton" toggleGroup="$answers" />
                <Button fx:id="submitButton" layoutX="156.0" layoutY="194.0" mnemonicParsing="false" onAction="#onSubmitButtonPressed" prefHeight="27.0" prefWidth="86.0" text="submit" />
                <TextField fx:id="userDialog" layoutX="311.0" layoutY="208.0" prefHeight="27.0" prefWidth="161.0" />
                <Label fx:id="question" layoutX="11.0" layoutY="14.0" prefHeight="72.0" prefWidth="532.0" text="Label" />
                <Button fx:id="continueButton" layoutX="156.0" layoutY="234.0" mnemonicParsing="false" onAction="#onContinueButtonPressed" prefHeight="27.0" prefWidth="86.0" text="continue" />
            </children>
        </AnchorPane>
    </children>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
</GridPane>
